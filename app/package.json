{
  "name": "ayobuenavista.github.io",
  "version": "2.0.0",
  "description": "My profile built using Svelte",
  "author": "Anton Buenavista <ay.buenavista@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayobuenavista/ayobuenavista.github.io.git"
  },
  "scripts": {
    "dev": "sapper dev",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.svelte src",
    "build:css": "tailwind build src/tailwind.css -c tailwind.config.js -o static/style.css",
    "build": "npm run build:css && sapper build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.4",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "svelte": "^3.20.1",
    "tailwindcss": "^1.3.1"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "font-awesome": "^4.7.0",
    "js-yaml": "^3.13.1",
    "material-design-icons": "^3.0.1",
    "particles.js": "^2.0.0",
    "polka": "^0.5.2",
    "roboto-fontface": "^0.10.0",
    "sirv": "^0.4.2",
    "sirv-cli": "^0.4.5"
  }
}
